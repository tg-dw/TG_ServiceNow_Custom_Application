        <description>시작 날짜는 어제보다 미래여야 한다.</description>
        <field>rent_date_start</field>
        <global>true</global>
        <isolate_script>true</isolate_script>
        <messages/>
        <name>OA rent_date_start Field Verify</name>
        <order/>
        <script><![CDATA[function onChange(control, oldValue, newValue, isLoading, isTemplate) {
   if (isLoading || newValue === '') {
      return;
   }

	//시작 날짜는 어제보다 미래여야 한다.
	var rent_start_date = new Date(g_form.getValue('rent_date_start'));
	
	var addDays = "1";
	
	var today_date = new Date();
	
	var today_days = today_date.getDate();
	
	var yesterday_date = parseInt(today_days) - parseInt(addDays);
	
	today_date.setDate(yesterday_date);
	
	var yesterday_date_time_str = formatDate(yesterday_date, g_user_date_format);
	
	if(yesterday_date_time_str > rent_start_date){
		g_form.showFieldMsg('rent_date_start','오늘 이후 날짜로 수정하시오.','error');
	}
}]]></script>
